{
  "extends": ["airbnb", "prettier", "eslint:recommended", "next"],
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".tsx"]}],
    "import/prefer-default-export": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "ts": "never",
        "tsx":"never"
      }
    ]
  }
}
